SELECT * FROM actors 
    JOIN movie_cast
    ON actors.id=movie_cast.actor_id; 

SELECT name, title FROM actors JOIN movies ON movies.id=movie_cast.movie_id JOIN movie_cast ON actors.id=movie_cast.actor_id;
